{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "npx @n24q02m/mnemo-plugin hook session-start",
            "timeout": 10
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "npx @n24q02m/mnemo-plugin hook stop",
            "timeout": 10
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "npx @n24q02m/mnemo-plugin hook pre-compact",
            "timeout": 15
          }
        ]
      }
    ]
  }
}
